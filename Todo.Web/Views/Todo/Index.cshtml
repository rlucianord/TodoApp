@model IEnumerable<TodoApi.Models.TodoItemDTO>

@{
    ViewBag.Title = "Taxes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}







    @foreach (var item in Model)
    {
          
            <div class="col-4">
                <div class="col-mr-1">
                    <a href="~/todo/EditItem/@item.Id" id="btnedit" class="dropdown-item">
                        Edit Registry
                    </a>
                </div>
                <div class="col-2"> @item.Id @@@item.Name @@@item.IsComplete</div>



            </div>





    }
